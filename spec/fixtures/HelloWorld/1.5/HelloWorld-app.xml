<?xml version="1.0" encoding="UTF-8"?>
<application xmlns="http://ns.adobe.com/air/application/1.5">
	<id>com.example.HelloWorld</id>
	<version>3.1.4</version>
	<filename>HelloWorld</filename>
	<name>Name</name>
	<description>
		<text xml:lang="en">Description en</text>
		<text xml:lang="fr">Description fr</text>
		<text xml:lang="es">Description es</text>
	</description>
	<copyright>Copyright</copyright>
	<icon>
		<image16x16>icons/icon_16x16.png</image16x16>
		<image32x32>icons/icon_32x32.png</image32x32>
		<image48x48>icons/icon_48x48.png</image48x48>
		<image128x128>icons/icon_128x128.png</image128x128>
	</icon>
	<fileTypes>
		<fileType>
			<name>HelloWorld.Text</name>
			<extension>hwtxt</extension>
			<description>HelloWorld text file description</description>
			<contentType>text/plain</contentType>
			<icon>
				<image16x16>iconsdoctxt/icon_16x16.png</image16x16>
				<image32x32>iconsdoctxt/icon_32x32.png</image32x32>
				<image48x48>iconsdoctxt/icon_48x48.png</image48x48>
				<image128x128>iconsdoctxt/icon_128x128.png</image128x128>
			</icon>
		</fileType>
		<fileType>
			<name>HelloWorld.Binary</name>
			<extension>hwbin</extension>
			<description>HelloWorld binary file description</description>
			<contentType>application/octet-stream</contentType>
			<icon>
				<image16x16>iconsdocbin/icon_16x16.png</image16x16>
				<image32x32>iconsdocbin/icon_32x32.png</image32x32>
				<image48x48>iconsdocbin/icon_48x48.png</image48x48>
				<image128x128>iconsdocbin/icon_128x128.png</image128x128>
			</icon>
		</fileType>
		<fileType>
			<name>HelloWorld.Binary2</name>
			<extension>hwbin2</extension>
			<description>HelloWorld binary file description 2</description>
			<contentType>application/octet-stream</contentType>
			<icon>
				<image16x16>iconsdocbin/icon_16x16.png</image16x16>
				<image32x32>iconsdocbin/icon_32x32.png</image32x32>
				<image48x48>iconsdocbin/icon_48x48.png</image48x48>
				<image128x128>iconsdocbin/icon_128x128.png</image128x128>
			</icon>
		</fileType>
	</fileTypes>
	<initialWindow>
		<title>Title</title>
		<content>HelloWorld.swf</content>
		<visible>true</visible>
	</initialWindow>
</application>
